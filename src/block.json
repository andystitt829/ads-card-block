{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/ads-card-block",
	"version": "0.1.0",
	"title": "Card Block",
	"category": "widgets",
	"icon": "id",
	"description": "Card pattern with an image, title, and description.",
	"supports": {
		"color": true
	},
	"attributes": {
		"imageAlt": {
			"type": "string",
			"attribute": "alt",
			"selector": ".card__image"
		},
		"imageUrl": {
			"type": "string",
			"attribute": "src",
			"selector": ".card__image"
		},
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	},
	"textdomain": "ads-card-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
